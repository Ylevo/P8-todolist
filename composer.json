{
    "name": "saro0h/to-do-list",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.0",
        "doctrine/orm": "^2.15.0",
        "doctrine/annotations": "^2.0",
        "symfony/monolog-bundle": "^v3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/flex": "^2",
        "symfony/form": "6.3.*",
        "symfony/asset": "6.3.*",
        "symfony/http-client": "6.3.*",
        "symfony/intl": "6.3.*",
        "symfony/doctrine-messenger": "6.3.*",
        "symfony/mailer": "6.2.*",
        "symfony/security-bundle": "6.2.*",
        "symfony/yaml": "6.3.*",
        "symfony/twig-bundle": "6.2.*",
        "twig/twig": "^v3.6.0",
        "symfony/runtime": "v6.3",
        "symfony/error-handler": "v6.3",
        "symfony/console": "v6.3",
        "symfony/validator": "v6.3",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "symfony/dotenv": "v6.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.3",
        "symfony/stopwatch": "v6.3",
        "symfony/web-profiler-bundle": "v6.3",
        "symfony/maker-bundle": "^1.50"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "v6.3"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
